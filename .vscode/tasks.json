{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build Generator.exe",
      "type": "shell",
      "command": ".\\build.bat",
      "group": {
        "kind": "build",
        "isDefault": true
      },
    },
    {
      "label": "run Generator.exe",
      "type": "shell",
      "command": "${workspaceFolder}/out/bin/Generate.exe",
      "args": ["--iso=${workspaceFolder}/out/Win11_24H2_English_x64.iso"],
      "group": {
        "kind": "test",
        "isDefault": false
      }
    }
  ],
}